<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.testing.generateReport_controller">
   <children>
      <Label layoutX="50.0" layoutY="50.0" text="Report Type" />
      <TextField fx:id="txtReportTypeID" layoutX="120.0" layoutY="45.0" prefWidth="200.0" />

      <Label layoutX="50.0" layoutY="113.0" text="Month" />
      <TextField fx:id="txtMonthID" layoutX="120.0" layoutY="108.0" prefWidth="200.0" />
      
      <Label layoutX="50.0" layoutY="170.0" text="Year" />
      <TextField fx:id="txtYearID" layoutX="120.0" layoutY="165.0" prefWidth="200.0" />
      <Button fx:id="btnGenerateReport" layoutX="150.0" layoutY="210.0" onAction="#handleGenerateReport" text="generate" />
   </children>
</AnchorPane>
